  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" \"dev/**/*.ts\"",
    "start": "nest start dev",
    "start:dev": "nest start dev --watch",
    "start:dev:debug": "nest start dev --debug --watch",
    "start:dev:debug:chrome": "tsc-watch -p tsconfig.build.json --onSuccess \"node --inspect-brk dist/dev/main.js\"",
    "start:debug": "nest start --debug --watch",
    "start:debug:chrome": "tsc-watch -p tsconfig.build.json --onSuccess \"node --inspect-brk dist/dist/main.js\"",
    "start:prod": "nest build && node dist/src/main",
    "builded:dev": "node dist/dev/main",
    "builded:prd": "node dist/src/main",
    "lint": "eslint \"{src,apps,libs,dev,test}/**/*.ts\"",
    "lint:fix": "eslint \"{src,apps,libs,dev,test}/**/*.ts\" --fix",
    "test": "jest --config ./test/jest.json",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:integration": "cucumber-js -p default",
    "artillery": "artillery"
  },
